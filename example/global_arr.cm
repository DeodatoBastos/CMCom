/* ordenacao por selecao */
int x[10];

void print_arr(int a[], int n) {
    int i;
    i = 0;
    while (i < n) {
        output(a[i]);
        i = i + 1;
    }
}

void main(void) {
    int i;
    i = 0;
    while(i < 10) {
        // x[i] = input();
        x[i] = 2 * i;
        i = i + 1;
    }
    print_arr(x, 10);
}
